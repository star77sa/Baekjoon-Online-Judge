   "source": [
    "N = int(input())\n",
    "count = 0\n",
    "\n",
    "while N != 1:\n",
    "    if (N%3 == 0) & (N>=3):\n",
    "        N = N / 3\n",
    "    elif (N%3 == 1):\n",
    "        N = N-1\n",
    "    elif (N%3 == 2) & (N % 2 == 0) : # 짝수\n",
    "        N = N/2\n",
    "    elif (N%3 == 2) & (N % 2 == 1) : # 홀수\n",
    "        N = N-1\n",
    "    count += 1\n",
    "        \n",
    "print(count)"
   ]
