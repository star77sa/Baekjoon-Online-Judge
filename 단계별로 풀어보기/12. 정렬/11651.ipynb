    "import sys\n",
    "N = int(sys.stdin.readline())\n",
    "cache = []\n",
    "for i in range(N):\n",
    "    cache.append(list(map(int, sys.stdin.readline().split())))\n",
    "cache.sort(key = lambda x: (x[1], x[0]))\n",
    "for i in range(N):\n",
    "    print(*cache[i])"
   ]
