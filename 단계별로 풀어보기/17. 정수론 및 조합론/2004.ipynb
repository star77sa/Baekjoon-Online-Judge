    "from math import factorial\n",
    "import sys\n",
    "\n",
    "#N, K = map(int,sys.stdin.readline().split())\n",
    "N, K = map(int,input().split())\n",
    "\n",
    "\n",
    "N = factorial(N)//(factorial(N-K)*factorial(K)) # nCk\n",
    "\n",
    "count = 0\n",
    "\n",
    "while N%10 == 0:\n",
    "    N = N//10\n",
    "    count += 1 \n",
    "    \n",
    "print(count)"
