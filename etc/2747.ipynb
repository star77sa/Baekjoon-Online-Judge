    "N = int(input())\n",
    "def Fib(N):\n",
    "    dp = [0 for i in range (N+1)]\n",
    "    dp[1] = 1\n",
    "    i = 2\n",
    "    for i in range(2,N+1):\n",
    "        dp[i] = dp[i-1] + dp[i-2]\n",
    "    return dp[N]\n",
    "\n",
    "print(Fib(N))"
   ]
